{
  "name":"atomy/iot-media-api",
  "description":"API for iot-media",
  "license": "MIT",
  "require":{
    "php": ">=7.4",
    "ext-json": "*",
    "monolog/monolog": "^2.1.1",
    "php-di/php-di": "^6.1",
    "slim/psr7": "^1.1",
    "slim/slim": "^4.5.0"
  },
  "require-dev": {
    "phpunit/phpunit": ">=9.3.6"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/App/",
      "IotMediaApi\\": "src/IotMediaApi/"
    }
  },
  "autoload-dev": {
    "psr-4":{
      "": "src/"
    }
  }
}
